@using KandQTicaret.Models
@model List<OzellikTip>
<div class="form-group">
    <label class="col-sm-2 control-label">Özellik Tip</label>
    <div class="col-sm-10 ">
        <select name="OzellikTipID" id="tip" class="form-control">
            <option value="0">Özellik Tip Seçiniz</option>
            @foreach (OzellikTip item in Model)
            {
                <option value="@item.Id">@item.Adi</option>
            }
        </select>
    </div>
</div>
